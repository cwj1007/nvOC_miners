
{
    "friendlyname": "z-enemy",
    "version": "1.28",
    "hcd": "  ccminer $UCCALGO -o ${!xproto}://${!xpool}:${!xport} -u ${!xaddr}${!xwallet}${!xwork} -p $MINER_PWD -i ${!xintensity} $CCMINER_OPTS ${!xext}",
    "devlist_argument": "--devices ",
    "devlist_separator": ",",
    "devlist_format": "N",
    
    "foreman":
    {
      "name": "z-enemy",
      "api_ext": "-b 127.0.0.1:4068"
    },

    "minerinfo":
    {
      "total_hash": "tail -100 ${NVOC}/nvoc_logs/screenlog.0 | grep "Shares" | grep "[M]H/s" | sed -r 's/\x1B\[([0-9]{1,2}(;[0-9]{1,2})?)?[m|K]//g' | grep -o -E "[0-9]*.[0-9]*MH/s" | sed 's:MH/s::' | awk '{s+=$1} END {printf "%.0f", s}'",      "hash_count": "tail -100 ${NVOC}/nvoc_logs/screenlog.0 | grep Speed | grep -v grep | wc -l | awk '{print $1}'",
      "solution_speed_unit": "MH/s",
      "miner_error": "false",
      "rejected_shares": "ail -n 100 ${NVOC}/nvoc_logs/screenlog.0 | grep "Shares:" | grep -o "R:[0-9]*.[0-9]*%" | cut -f2- -d: | tail -1"
    },

    "install":
    {
      "latest": "true",
      "recommended": "cuda-9.2",
      "tarball": "z-enemy-1.28-cuda92.tar.xz",
      "executable": "ccminer"
    }
}
